{
  "name": "drenso/symfony-deployer-bundle",
  "description": "Deploy scripting bundle for Symfony",
  "keywords": [
    "symfony",
    "deploy",
    "deployment",
    "scripting"
  ],
  "type": "symfony-bundle",
  "homepage": "https://gitlab.drenso.nl/intern/symfony-deployer-bundle",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Bob van de Vijver",
      "email": "bob@drenso.nl"
    },
    {
      "name": "Tobias Feijten",
      "email": "tobias@drenso.nl"
    }
  ],
  "require": {
    "php": ">=8.1",
    "doctrine/doctrine-bundle": "^2.7",
    "doctrine/orm": "^2.13",
    "symfony/config": "^5.4|^6.0",
    "symfony/dependency-injection": "^5.4|^6.0",
    "symfony/http-kernel": "^5.4|^6.0",
    "symfony/validator": "^5.4|^6.0"
  },
  "require-dev": {
    "drenso/phan-extensions": "^3.0",
    "friendsofphp/php-cs-fixer": "3.8.0",
    "rector/rector": "0.14.2",
    "symfony/messenger": "^5.4|^6.0",
    "symfony/twig-bundle": "^5.4|^6.0",
    "twig/twig": "^3.4"
  },
  "autoload": {
    "psr-4": {
      "Drenso\\DeployerBundle\\": "src"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "v0.x-dev"
    }
  }
}
